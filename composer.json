{
    "name": "oxenti/cake_gcm",
    "type": "cakephp-plugin",
    "description": "CakeGCM is a plugin for CakePHP to send notification to an Android device through Google Cloud Messaging. This project is a fork from ker0x's repo.",
    "homepage": "https://github.com/adewaleandrade/CakeGCM",
    "keywords": [
        "cakephp",
        "google",
        "cloud",
        "messaging"
    ],
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Romain Monteil",
            "email": "monteil.romain@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/ker0x/CakeGCM/issues",
        "source": "https://github.com/ker0x/CakeGCM"
    },
    "require": {
        "cakephp/cakephp": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "CakeGcm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeGcm\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
